{
  "name": "pi-session-ask",
  "version": "0.1.1",
  "description": "Allows the agent or user to ask questions about the current session's pre-compaction history, parent (if forked) sessions' histories, or any other sessions' histories, via a subagent (keeping current context clean).",
  "keywords": ["pi-package", "pi", "pi-coding-agent", "session", "compaction"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w-winter/dot314.git",
    "directory": "packages/pi-session-ask"
  },
  "bugs": {
    "url": "https://github.com/w-winter/dot314/issues"
  },
  "homepage": "https://github.com/w-winter/dot314#readme",
  "pi": {
    "extensions": ["extensions/session-ask/index.ts"]
  },
  "peerDependencies": {
    "@mariozechner/pi-coding-agent": ">=0.51.0",
    "@mariozechner/pi-tui": "*",
    "@sinclair/typebox": "*"
  },
  "scripts": {
    "prepack": "node ../../scripts/pi-package-prepack.mjs"
  },
  "files": ["extensions/**", "README.md", "LICENSE", "package.json"],
  "dot314Prepack": {
    "copy": [
      { "from": "../../extensions/session-ask/index.ts", "to": "extensions/session-ask/index.ts" },
      { "from": "../../extensions/session-ask/config.json.example", "to": "extensions/session-ask/config.json.example" },
      { "from": "../../LICENSE", "to": "LICENSE" }
    ]
  }
}
