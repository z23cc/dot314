{
  "name": "pi-move-session",
  "version": "0.2.1",
  "description": "Move a live Pi session to a different working directory without losing context and jump into the new session",
  "keywords": ["pi-package", "pi", "pi-coding-agent", "session"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w-winter/dot314.git",
    "directory": "packages/pi-move-session"
  },
  "bugs": {
    "url": "https://github.com/w-winter/dot314/issues"
  },
  "homepage": "https://github.com/w-winter/dot314#readme",
  "pi": {
    "extensions": ["extensions/move-session.ts"]
  },
  "peerDependencies": {
    "@mariozechner/pi-coding-agent": ">=0.51.0"
  },
  "scripts": {
    "prepack": "node ../../scripts/pi-package-prepack.mjs"
  },
  "files": ["extensions/**", "README.md", "LICENSE", "package.json"],
  "dot314Prepack": {
    "copy": [
      { "from": "../../extensions/move-session.ts", "to": "extensions/move-session.ts" },
      { "from": "../../LICENSE", "to": "LICENSE" }
    ]
  }
}
